hi clear
syntax reset
set background=dark
let g:colors_name = "homebrew"

" Define colors based on Homebrew Terminal theme
hi Normal guifg=#e5e9f0 guibg=#1c1c1c
hi Comment guifg=#999999 gui=italic
hi Constant guifg=#ffd700
hi Identifier guifg=#87d7ff
hi Statement guifg=#ffaf87
hi PreProc guifg=#ff5f5f
hi Type guifg=#87ff87
hi Special guifg=#5fafaf
hi Underlined guifg=#af87ff gui=underline
hi Error guifg=#ff0000 guibg=#1c1c1c
hi Todo guifg=#ffd7af guibg=#1c1c1c gui=bold
