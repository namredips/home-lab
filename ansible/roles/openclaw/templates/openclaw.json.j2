{
  "agents": {
    "defaults": {
      "workspace": "{{ openclaw_install_dir }}/workspace",
      "model": {
        "primary": "ollama/{{ ollama_primary_model }}"
      }
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "identity": {
          "name": "{{ agent_name }}",
          "email": "{{ agent_email }}"
        }
      }
    ]
  },
  "models": {
    "providers": {
      "ollama": {
        "api": "openai-completions",
        "apiKey": "ollama-local",
        "baseUrl": "http://127.0.0.1:11434/v1",
        "models": [
{% for model in ollama_cloud_models %}
          {
            "contextWindow": 131072,
            "cost": {
              "cacheRead": 0,
              "cacheWrite": 0,
              "input": 0,
              "output": 0
            },
            "id": "{{ model }}",
            "input": [
              "text"
            ],
            "maxTokens": 16384,
            "name": "{{ model }}",
            "reasoning": false
          }{{ "," if not loop.last else "" }}
{% endfor %}
        ]
      }
    }
  },
  "channels": {
    "discord": {
      "enabled": true,
      "guildId": "{{ discord_guild_id }}",
      "botToken": "${DISCORD_BOT_TOKEN}",
      "defaultChannel": "general"
    }
  },
  "gateway": {
    "mode": "local",
    "port": {{ openclaw_dashboard_port }}
  },
  "tools": {
    "profile": "full"
  },
  "browser": {
    "headless": true
  },
  "logging": {
    "level": "{{ openclaw_log_level }}",
    "path": "{{ openclaw_install_dir }}/logs"
  }
}
