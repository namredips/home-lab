# OpenClaw Environment Variables for {{ agent_name }}
# Store API keys here - this file should be kept secure (mode 0600)

# Anthropic API key (for Claude Code)
ANTHROPIC_API_KEY={{ openclaw_anthropic_api_key }}

# OpenAI API key (for Codex/GPT)
OPENAI_API_KEY={{ openclaw_openai_api_key }}

# Google API key (for Gemini)
GOOGLE_API_KEY={{ openclaw_google_api_key }}

# Mattermost bot token
MATTERMOST_BOT_TOKEN={{ mattermost_bot_token }}

# Agent identification
AGENT_NAME={{ agent_name }}
AGENT_EMAIL={{ agent_email }}
